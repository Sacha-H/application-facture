<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Mes Factures{% endblock %}</title>
        <link rel="shortcut icon" href="{{ asset('images/PorgForceFriday.ico')}}" type="image/x-icon">
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            {# CSS #}
			<link rel="stylesheet" href="{{ asset('css/style.css')}}">
            <script src="https://kit.fontawesome.com/a3ab995fc5.js" crossorigin="anonymous"></script>
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body>
        {% block body %}{% endblock %}
    </body>
</html>
